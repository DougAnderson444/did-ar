{
	"name": "@peerpiper/did-ar",
	"version": "0.0.17",
	"main": "index.js",
	"module": "index.js",
	"scripts": {
		"dev": "vite dev",
		"arlocal": "npx arlocal",
		"build": "vite build",
		"build:components": "npm run package && node buildComponents.mjs",
		"preview": "vite preview",
		"prepare": "ts-patch install -s",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage",
		"package": "svelte-kit sync && svelte-package && npm run purge:package",
		"purge:package": "node purge.js",
		"publish:package": "npm publish ./package --access public",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@peerpiper/web3-wallet-connector": "^0.1.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.5",
		"@vitest/ui": "^0.24.0",
		"arlocal": "^1.1.58",
		"autoprefixer": "^10.4.12",
		"globby": "^13.1.2",
		"postcss": "^8.4.17",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"process": "^0.11.10",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"ts-patch": "^2.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"typescript-transform-extensions": "^1.0.1",
		"vite": "^3.1.0",
		"vitest": "^0.23.4"
	},
	"type": "module",
	"dependencies": {
		"@stablelib/base64": "^1.0.1",
		"ar-gql": "github:DougAnderson444/arGQL",
		"did-resolver": "^4.0.0",
		"multiformats": "^9.9.0",
		"svelte-json-tree": "^1.0.0",
		"warp-contracts": "^1.2.12"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DougAnderson444/did-ar.git"
	}
}
